{
  "name": "vragovr/notification-bundle",
  "description": "Simple email notifications bundle for Symfony.",
  "version": "0.0.1",
  "type": "library",
  "keywords": [
    "symfony",
    "bundle",
    "swiftmailer",
    "email"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.0",
    "doctrine/collections": "~1.4",
    "symfony/http-kernel": "~3.3|~4.0",
    "symfony/dependency-injection": "~3.3|~4.0",
    "symfony/config": "~3.3|~4.0",
    "symfony/swiftmailer-bundle": "^3.1",
    "symfony/twig-bundle": "~3.3|~4.0",
    "tijsverkoyen/css-to-inline-styles": "~2.0",
    "html2text/html2text": "~4.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "NotificationBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "NotificationBundle\\": "tests/"
    }
  }
}
